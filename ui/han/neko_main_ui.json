{
  "namespace": "rem",

  "exit_icon": {
    "type": "image",
    "size": [ 18 , 18 ],
    "texture": "textures/ui-x/icon-new/exit_icon"
  },

  "panel_icon": {
    "type": "panel",
    "size": [ "100%" , "30px - 2px"]
  },

  "panel_icon_2": {
    "type": "panel",
    "size": [ "fill" , "100%"]
  },

  "panel_icon_3": {
    "type": "panel",
    "size": [ "100%" , "34px"]
  },

  "panel_icon_4": {
    "type": "panel",
    "size": [ "100%" , "28px"]
  },
  
  "player_name@rem.bg_2": {
    "size": [ "100% - 200px" , 20 ],
    "max_size": [ "100% - 205px", 20 ],
    "anchor_from": "bottom_right",
    "anchor_to": "bottom_right",
    "offset": [ -5 , -7 ],
    "controls": [
      {
       "stack_panel_0": {
        "type": "stack_panel",
        "orientation": "horizontal",
        "size": [ "100%" , "100%" ],
        "anchor_from": "left",
        "anchor_to": "left",
        "controls": [
          { "panel_icon_2@rem.panel_icon_2": {"controls":[{ "icon1@neko_button.achievements_button": {}}]}},
          { "panel_icon_2@rem.panel_icon_2": {"controls":[{ "icon1@neko_button.store_button": {}}]}},
          { "panel_icon_2@rem.panel_icon_2": {"controls":[{ "icon1@neko_button.profile_button": {}}]}},
          { "panel_icon_2@rem.panel_icon_2": {"controls":[{ "icon1@neko_button.settings_button": {}}]}}
          ]
        }
      }
    ]
  },

  "button_dan_icon": {
    "type": "panel",
    "size": [ 110 , "100%" ],
    "anchor_from": "left_middle",
    "anchor_to": "left_middle",
    "offset": [ 5 , -5 ],
    "controls": [
      {
       "stack_panel_0": {
        "type": "stack_panel",
        "size": [ "100%" , "100%c" ],
        "orientation": "vertical",
        "anchor_from": "bottom_left",
        "anchor_to": "bottom_left",
        "animation_reset_name": "screen_animation_reset",
        "anims": [
          "@anim_test.screen_exit_animation_push_offset",
          "@anim_test.screen_exit_animation_pop_offset",
          "@anim_test.screen_entrance_animation_push_offset",
          "@anim_test.screen_entrance_animation_pop_offset"
        ],
        "controls": [
          { "panel_icon_1@rem.panel_icon_4": {"controls": [ { "xbox@rem.xbox": {}}], "bindings": [{ "binding_name": "#sign_in_visible", "binding_name_override": "#visible" }]}},
          { "panel": { "type": "panel" , "size": [ "100%" , "4px" ]}},
          { "panel_icon_1@rem.panel_icon": {"controls": [ { "panel_nama_player@loli.panel_nama_player": {}}]}},
          { "panel_icon_2@rem.panel_icon_3": {"controls":[{ "icon1@neko_button.play_button": {}}]}}
          ]
        }
      }
    ]
  },

  "bg_2": {
    "type":  "image",
    "alpha": 0.75,
    "texture": "textures/ui-x/dialog/bg",
    "size": [ "100%" , "100%" ]
  },

  "bg_3": {
    "type":  "image",
    "alpha": 0.75,
    "texture": "textures/ui-x/dialog/event_bg",
    "size": [ "100%" , "100%" ]
  },

  "xbox": {
    "type": "panel",
    "controls": [
      {
        "xbox_panel": {
          "type": "stack_panel",
          "size": [ "100%" , "100%" ],
          "orientation" : "horizontal",
          "anchor_from": "left_middle",
          "anchor_to": "left_middle",
          "controls": [
            {
              "xbox_icon":{
                "type": "panel",
                "anchor_from": "center",
                "anchor_to": "center",
                "size": [ "30px" , "100%" ],
                "controls": [
                  {
                    "xbox_icon_0": {
                      "type": "image",
                      "layer": 2,
                      "texture": "textures/ui-x/icon-new/warn",
                      "size": [ 16 , 16 ]
                    }
                  }
                ]
              }
            },
            {
              "xbox_label":{
                "type": "panel",
                "size": [ "fill" , "100%" ],
                "controls": [
                  {
                    "xbox_label_0": {
                      "type": "label",
                      "anchor_from": "middle_left",
                      "anchor_to": "middle_left",
                      "layer": 2,
                      "font_size": "small",
                      "size": [ "100%" , "default" ],
                      "text": "Minecraft kamu belum\nlogin xbox :("
                    }
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },

    /// Button Profile , Skins , Feedback
		
    "feedback_button@common_buttons.light_content_button": {
    "$default_button_texture|default": "",
    "$hover_button_texture|default": "",
    "$pressed_button_texture|default": "",
		"$button_content": "fnx.feedback_icon",
    "size": [ 24 , 24 ],
    "$light_border_default_color": [ 0.0 , 0.0 , 0.0 ],
    "$light_border_hover_color": [ 0.09, 0.09, 0.09 ],
        "controls": [
         { "edu_feedback_button@start.edu_feedback_button": { "ignored": "(not $education_edition)" } },
         { "release_feedback_button@start.release_feedback_button": { "ignored": "($pre_release or $education_edition)" } },
         { "pre_release_feedback_button@start.pre_release_feedback_button": { "ignored": "(not $pre_release or $education_edition)" } }
       ]
    },

    "exit_button@common_buttons.light_content_button": {
    "$default_state_border_visible": false,
    "$hover_state_border_visible": false,
    "$pressed_state_border_visible": false,
    "$locked_state_border_visible": false,
    "$default_button_texture|default": "",
    "$hover_button_texture|default": "",
    "$pressed_button_texture|default": "",
		"$button_content": "rem.exit_icon",
    "$pressed_button_name": "button.menu_exit",
    "size": [ 24 , 24 ],
    "$light_border_default_color": [ 0.0 , 0.0 , 0.0 ],
    "$light_border_hover_color": [ 1.0, 1.0, 1.0 ]
    },

    "stacked_row_0": {
      "type": "panel",
      "size": [ "27px" , "24px" ]
    },

    "button_panel_1": {
      "type": "panel",
      "anchor_from": "top_right",
      "anchor_to": "top_right",
      "size": [ "100%" , 24 ],
      "offset": [ -5 , 5 ],
      "controls": [
        {
          "stack_button_panel_1": {
            "type" : "stack_panel",
            "anchor_from": "top_right",
            "anchor_to": "top_right",
            "orientation": "horizontal",
            "controls": [
              {
                "stacked_row_0@rem.stacked_row_0": { "controls": [{ "feedback_button@rem.feedback_button": { "ignored": "$feedback_ignored", "bindings": [ {"binding_name": "#feedback_visible","binding_name_override": "#visible"}]}}]}
              },
              {
                "stacked_row_0@rem.stacked_row_0": { "controls": [{ "exit_button@rem.exit_button": {}}]}
              }
            ]
          }
        }
      ]
    },

  "skin_panel": {
    "type": "panel",
    "anchor_to": "middle",
    "anchor_from": "middle",
    "size": [ 240 , 240 ],
    "offset": [ 0 , 0 ],
    "controls": [
      { "viewer_panel@start.skin_viewer_panel": {} }
    ]
  },

  "button_panel": {
  "type": "panel",
  "controls": [      
        { "version_panel@loli.version_panel": {}},
        { "skin_panel@rem.skin_panel": {}},
        { "button_panel_1@rem.button_panel_1": {}},
        { "player_name@rem.player_name": {}},
        { "button_dan_icon@rem.button_dan_icon": {}}
      ]
    }
  }
